                              <object class="GtkLabel" id="search_mode_label">
                                <property name="visible">True</property>
                                <property name="use-underline">True</property>
                                <property name="mnemonic-widget">search_mode_button</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkImage">
                                <property name="visible">True</property>
                                <property name="icon-name">pan-down-symbolic</property>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkComboBoxText" id="room_search_combobox">
                        <property name="visible">False</property>
                        <property name="has-entry">True</property>
                        <items>
                          <item translatable="yes" id="joined">Joined Rooms </item>
                        </items>
                        <property name="active-id">joined</property>
                        <child internal-child="entry">
                          <object class="GtkEntry" id="room_search_entry">
                            <property name="hexpand">False</property>
                            <property name="max-width-chars">20</property>
                            <property name="width-chars">5</property>
                            <property name="placeholder-text" translatable="yes">Room…</property>
                            <signal name="activate" handler="on_search"/>
                          </object>
                        </child>
                        <style>
                          <class name="dropdown-scrollbar"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="GtkComboBoxText" id="user_search_combobox">
                        <property name="visible">False</property>
                        <property name="has-entry">True</property>
                        <child internal-child="entry">
                          <object class="GtkEntry" id="user_search_entry">
                            <property name="hexpand">False</property>
                            <property name="max-width-chars">20</property>
                            <property name="width-chars">5</property>
                            <property name="placeholder-text" translatable="yes">Username…</property>
                            <signal name="activate" handler="on_search"/>
                          </object>
                        </child>
                        <style>
                          <class name="dropdown-scrollbar"/>
                        </style>
                      </object>
