    <property name="visible">True</property>
    <property name="tooltip-text" translatable="yes">Menu</property>
  </object>
  <object class="GtkHeaderBar" id="header_search">
    <property name="visible">True</property>
    <property name="height-request">42</property>
    <child type="title">
      <object class="GtkBox" id="search_title">
        <property name="visible">True</property>
        <property name="spacing">4</property>
        <property name="valign">center</property>
        <child>
          <object class="GtkMenuButton" id="SearchMode">
            <property name="visible">True</property>
            <property name="height-request">0</property>
            <property name="tooltip-text" translatable="yes">Search scope</property>
          </object>
        </child>
        <child>
          <object class="GtkComboBoxText" id="RoomSearchCombo">
            <property name="visible">False</property>
            <property name="height-request">0</property>
            <property name="has-entry">True</property>
            <items>
              <item translatable="yes" id="joined">Joined Rooms </item>
            </items>
            <property name="active-id">joined</property>
            <child internal-child="entry">
              <object class="GtkEntry" id="RoomSearchEntry">
                <property name="max-width-chars">20</property>
                <property name="width-chars">5</property>
                <property name="placeholder-text" translatable="yes">Room…</property>
                <signal name="activate" handler="on_search" swapped="no"/>
              </object>
            </child>
            <child internal-child="button">
              <object class="GtkButton">
                <signal name="clicked" handler="focus_combobox" swapped="no"/>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkComboBoxText" id="UserSearchCombo">
            <property name="visible">False</property>
            <property name="height-request">0</property>
            <property name="has-entry">True</property>
            <child internal-child="entry">
              <object class="GtkEntry" id="UserSearchEntry">
                <property name="max-width-chars">20</property>
                <property name="width-chars">5</property>
                <property name="placeholder-text" translatable="yes">Username…</property>
                <signal name="activate" handler="on_search" swapped="no"/>
              </object>
            </child>
            <child internal-child="button">
              <object class="GtkButton">
                <signal name="clicked" handler="focus_combobox" swapped="no"/>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkComboBoxText" id="SearchCombo">
            <property name="visible">True</property>
            <property name="height-request">0</property>
            <property name="has-entry">True</property>
            <child internal-child="entry">
              <object class="GtkEntry" id="SearchEntry">
                <property name="max-width-chars">75</property>
                <property name="width-chars">5</property>
                <property name="placeholder-text" translatable="yes">Search term…</property>
                <property name="tooltip-text" translatable="yes">Search patterns: with a word = term, without a word = -term, partial word = *erm</property>
                <property name="primary-icon-name">system-search-symbolic</property>
                <signal name="activate" handler="on_search" swapped="no"/>
                <signal name="icon-press" handler="on_search" swapped="no"/>
              </object>
            </child>
            <child internal-child="button">
              <object class="GtkButton">
                <signal name="clicked" handler="focus_combobox" swapped="no"/>
              </object>
            </child>
          </object>
        </child>
