                                        <property name="valign">center</property>
                                        <child>
                                          <object class="GtkBox">
                                            <property name="visible">True</property>
                                            <property name="spacing">6</property>
                                            <property name="margin-start">6</property>
                                            <property name="margin-end">6</property>
                                            <child>
                                              <object class="GtkLabel">
                                                <property name="visible">True</property>
                                                <property name="label" bind-source="_upload_users_button" bind-property="tooltip-text" bind-flags="bidirectional|sync-create"/>
                                                <attributes>
                                                  <attribute name="weight" value="bold"/>
                                                </attributes>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkButton" id="_upload_users_button">
                                                <property name="visible">True</property>
                                                <property name="tooltip-text" translatable="yes">Users</property>
                                                <signal name="clicked" handler="on_transfer_statistics"/>
                                                <child>
                                                  <object class="GtkLabel" id="upload_users_label">
                                                    <property name="visible">True</property>
                                                    <property name="label">0</property>
                                                    <property name="mnemonic-widget">_upload_users_button</property>
                                                    <attributes>
                                                      <attribute name="weight" value="bold"/>
                                                    </attributes>
                                                    <style>
                                                      <class name="dim-label"/>
                                                    </style>
                                                  </object>
                                                </child>
                                                <style>
                                                  <class name="circular"/>
                                                  <class name="count"/>
                                                </style>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkBox">
                                            <property name="visible">True</property>
                                            <property name="spacing">6</property>
                                            <property name="margin-start">6</property>
                                            <property name="margin-end">18</property>
                                            <child>
                                              <object class="GtkLabel">
                                                <property name="visible">True</property>
                                                <property name="label" bind-source="_upload_files_button" bind-property="tooltip-text" bind-flags="bidirectional|sync-create"/>
                                                <attributes>
                                                  <attribute name="weight" value="bold"/>
                                                </attributes>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkButton" id="_upload_files_button">
                                                <property name="visible">True</property>
                                                <property name="tooltip-text" translatable="yes">Files</property>
                                                <signal name="clicked" handler="on_transfer_statistics"/>
                                                <child>
                                                  <object class="GtkLabel" id="upload_files_label">
                                                    <property name="visible">True</property>
                                                    <property name="label">0</property>
                                                    <property name="mnemonic-widget">_upload_files_button</property>
                                                    <attributes>
                                                      <attribute name="weight" value="bold"/>
                                                    </attributes>
                                                    <style>
                                                      <class name="dim-label"/>
                                                    </style>
                                                  </object>
                                                </child>
                                                <style>
                                                  <class name="circular"/>
                                                  <class name="count"/>
                                                </style>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkBox" id="uploads_end">
                                        <property name="visible">True</property>
                                        <property name="spacing">6</property>
                                        <property name="valign">center</property>
