#: pynicotine/__init__.py:37
#, python-format
msgid "Website: %s"
msgstr "Website: %s"

#: pynicotine/__init__.py:43
msgid "show this help message and exit"
msgstr "mostrar esta mensagem de ajuda e sair"

#: pynicotine/__init__.py:46
msgid "file"
msgstr "arquivo"

#: pynicotine/__init__.py:47
msgid "use non-default configuration file"
msgstr "usar arquivo de configuração não padrão"

#: pynicotine/__init__.py:50
msgid "dir"
msgstr "pst"

#: pynicotine/__init__.py:51
msgid "alternative directory for user data and plugins"
